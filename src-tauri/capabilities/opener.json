{
  "$schema": "https://schema.tauri.app/config/2/capability",
  "identifier": "opener-restricted",
  "description": "Opener isolated for OAuth browser launch only",
  "windows": [
    "main"
  ],
  "remote": {
    "urls": [
      "https://dev.mywallpaper.online/*",
      "https://app.mywallpaper.online/*"
    ]
  },
  "permissions": [
    "opener:default"
  ]
}
